      "source": [
        "#approch 1  O(n^2)\n",
        "def sum_pair(arr,k):\n",
        "  for i in range(len(arr)):\n",
        "    for j in range(i+1,len(arr)):\n",
        "      if arr[i] + arr[j] == k:\n",
        "        return arr[i],arr[j]\n",
        "  return False\n",
        "  \n"
      ],
      "execution_count": 0,
      "outputs": []
