        "import math\n",
        "def maj(arr):\n",
        "  for i in range(math.ceil((len(arr)/2)-1)):\n",
        "    if arr[i] == arr[i+(math.ceil(len(arr)/2)-1)]:\n",
        "      return arr[i]\n",
        "  return False"
      ],
